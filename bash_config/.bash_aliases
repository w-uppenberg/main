# Aliases
alias sge="source gitenv.sh"
alias co="git checkout"
alias fixMerge="git diff --name-only --diff-filter=U | xargs code"
alias disk="du -h -d 1 | sort -hr"
alias _grep="grep -rin"
alias gitreset="git fetch && git reset --hard FETCH_HEAD"
alias tpr="tput reset"
alias ll='ls -laFq'
alias gs='git status'
alias gd='git diff'
alias repo='cd ~/repo'
alias home='cd ~/home'
